{
  "name": "splitfire-agency/laravel-paybox",
  "description": "Laravel 8 Paybox",
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^8.0",
    "lib-openssl": "*",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "autoload": {
    "psr-4": {
      "Sf\\PayboxGateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "license": "MIT",  
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "mockery/mockery": "^1.3.1",
    "ext-dom": "*",
    "ext-openssl": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "Sf\\Paybox\\Providers\\PayboxServiceProvider"
      ]
    }
  }
}
